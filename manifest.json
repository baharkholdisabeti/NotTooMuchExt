{
	"name": "Not Too Much",
	"version": "1.0",
	"description": "Encourages productivity by limiting distractions",
	"background": {
      		"scripts": ["background.js"],
      		"persistent": false
    	},

	"permissions": ["declarativeContent", "storage", "tabs","<all_urls>"],
	"options_page": "options.html",
	"page_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icon.png",
			"48": "icon.png",
			"128": "icon.png"
		}
	},
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
        "manifest_version": 2
}
